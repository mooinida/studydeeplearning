   "source": [
    "import tensorflow as tf\n",
    "x=[1,2,3,4,5]\n",
    "y=[1,2,3,4,5]\n",
    "w=tf.Variable(2,9)\n",
    "b=tf.Variable(0,5)\n",
    "\n",
    "h=w*x+b\n",
    "cost=tf.reduce_mean(tf.square(h-y))\n"
   ]
  },
